<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.curso.pruebadegui.Ventana_Agregar_Paquete">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Numero del paquete:"
        android:id="@+id/textView8"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editID"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:inputType="number" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Dimensiones del paquete"
        android:id="@+id/textView9"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/editID"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Ancho:"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editAncho"
        android:layout_below="@+id/textView10"
        android:layout_alignRight="@+id/buttonAgregarPaquete"
        android:layout_alignEnd="@+id/buttonAgregarPaquete" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Largo:"
        android:id="@+id/textView11"
        android:layout_below="@+id/editAncho"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editLargo"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/editAncho"
        android:layout_alignStart="@+id/editAncho" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Alto:"
        android:id="@+id/textView12"
        android:layout_below="@+id/editLargo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editAlto"
        android:layout_below="@+id/textView12"
        android:layout_alignLeft="@+id/editLargo"
        android:layout_alignStart="@+id/editLargo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar paquete"
        android:id="@+id/buttonAgregarPaquete"
        android:onClick="onClick"
        android:layout_below="@+id/editAlto"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="centimetros"
        android:id="@+id/textView2"
        android:layout_alignBottom="@+id/editAncho"
        android:layout_toRightOf="@+id/editAncho"
        android:layout_toEndOf="@+id/editAncho" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="centimetros"
        android:id="@+id/textView14"
        android:layout_alignBottom="@+id/editLargo"
        android:layout_toRightOf="@+id/editLargo"
        android:layout_toEndOf="@+id/editLargo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="centimetros"
        android:id="@+id/textView15"
        android:layout_alignBottom="@+id/editAlto"
        android:layout_toRightOf="@+id/editAlto"
        android:layout_toEndOf="@+id/editAlto" />

</RelativeLayout>
