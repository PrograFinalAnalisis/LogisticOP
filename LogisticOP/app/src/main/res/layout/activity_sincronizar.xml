<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.logisticop.logisticop.logisticop.Sincronizar"
    android:background="@drawable/v52"
    >

    <ListView
        android:id="@+id/SCHEDULE"
        android:layout_width="750dp"
        android:layout_height="550dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/lbNombreEmpleado"
        android:layout_above="@+id/btnSincronizar">
    </ListView>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Deseleccionar"
        android:id="@+id/btnDeseleccionar"
        android:layout_alignParentBottom="true"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
         android:text="@string/TituloSincronizar"
        android:id="@+id/lbTituloSincronizar"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TraerDatos"
        android:id="@+id/btnSincronizar"
        android:onClick="BtnMisActividades"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/presionado"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:textSize="21dp"
        android:layout_alignRight="@+id/lbact_registrada"
        android:layout_alignEnd="@+id/lbact_registrada"
        android:layout_above="@+id/btnDeseleccionar" />

    <TextView
        style="@style/HeaderOtro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/nombrempleado"
        android:id="@+id/lbNombreEmpleado"
        android:layout_below="@+id/lbTituloSincronizar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        style="@style/HeaderOtro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/act_registrada"
        android:id="@+id/lbact_registrada"
        android:layout_alignBaseline="@+id/lbNombreEmpleado"
        android:layout_alignBottom="@+id/lbNombreEmpleado"
        android:layout_alignRight="@+id/lbTituloSincronizar"
        android:layout_alignEnd="@+id/lbTituloSincronizar" />

    <TextView
        style="@style/HeaderOtro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/horas"
        android:id="@+id/lbhoras"
        android:layout_above="@+id/SCHEDULE"
        android:layout_alignRight="@+id/SCHEDULE"
        android:layout_alignEnd="@+id/SCHEDULE"
        android:layout_marginRight="24dp"
        android:layout_marginEnd="24dp" />

    <TextView
        style="@style/HeaderOtro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/fecha"
        android:id="@+id/lbfecha"
        android:layout_above="@+id/SCHEDULE"
        android:layout_toRightOf="@+id/lbact_registrada"
        android:layout_toEndOf="@+id/lbact_registrada"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignTop="@+id/SCHEDULE"
        android:layout_toLeftOf="@+id/lbact_registrada"
        android:layout_toStartOf="@+id/lbact_registrada"
        android:layout_marginTop="154dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/textView9"
        android:visibility="invisible"
        android:layout_above="@+id/btnDeseleccionar" />

    <Button
        android:layout_width="95dp"
        android:layout_height="45dp"
        android:text="@string/enviarDatos"
        android:id="@+id/button2"
        android:onClick="BtnEnviar"
        android:background="@drawable/presionado"
        android:textSize="21dp"
        android:layout_alignTop="@+id/btnTodas"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/btnEnviarTodas"
        android:layout_alignStart="@+id/btnEnviarTodas" />

    <Button
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:text="@string/traertodos"
        android:id="@+id/btnTodas"
        android:onClick="BtnTodasActividades"
        android:background="@drawable/presionado"
        android:textSize="21dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/btnSincronizar"
        android:layout_alignStart="@+id/btnSincronizar"
        android:layout_toLeftOf="@+id/lbfecha"
        android:layout_toStartOf="@+id/lbfecha" />

    <Button
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:text="@string/btnenviarTodas"
        android:id="@+id/btnEnviarTodas"
        android:onClick="enviarTodasAct"
        android:background="@drawable/presionado"
        android:textSize="21dp"
        android:layout_below="@+id/SCHEDULE"
        android:layout_alignRight="@+id/SCHEDULE"
        android:layout_alignEnd="@+id/SCHEDULE"
        android:layout_alignLeft="@+id/lbfecha"
        android:layout_alignStart="@+id/lbfecha" />

</RelativeLayout>
